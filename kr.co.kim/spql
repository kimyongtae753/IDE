SELECT * FROM EVENT BETWEEN 1 AND 2 order by EIDX DESC;

SELECT * FROM EVENT
//ROWNUM : 단순 SQL ROW의 번호를 정하는 명령어 (ORDER BY 적용안됨)
//ROW_NUMBER() : ORACLE에 전용 ROW함수를 설정하여 사용하는 함수(ORDER BY 적용)

SELECT ROWNUM AS NUM, EVENT.* FROM EVENT;

SELECT * FROM (SELECT ROWNUM NUM, EVENT.* FROM EVENT) WHERE NUM BETWEEN 1 AND 2;

//BETWEEN 시작값 AND 종료값 (1 ~ )
//SELECT * FROM EVENT 에서 나오는 순서로 적용된다

SELECT * FROM (SELECT ROW_NUMBER() OVER(ORDER BY EIDX DESC) NUM, EVENT.* FROM EVENT ORDER BY EIDX DESC)
WHERE NUM BETWEEN 1 AND 2;

